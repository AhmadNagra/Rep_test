@model mvc_app.Models.OutputClass

@{
    ViewData["Title"] = "outputView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>outputView</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.StudentList) { 
            <tr>
                <td>
                    <h4> @item.name  @item.program @item.detail </h4>
                </td>
            </tr>
        }
    </tbody>
</table>
